
@{
    ViewBag.Title = "UyeEkle";
    Layout = "~/Areas/Panel/Views/Shared/_LayoutPanel.cshtml";
}

<div class="box box-info">
    <div class="box-header with-border">
        <h3 class="box-title">Üye Ekleme Formu</h3>
    </div>
    <!-- /.box-header -->
    <!-- form start -->
    <form class="form-horizontal" action="/PanelUye/Ekle" method="post" enctype="multipart/form-data">
        <div class="box-body">

            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Üye Adı</label>
                <div class="col-sm-10">
                    <input type="text" name="UyeAdi" class="form-control" id="exampleInputEmail2" placeholder="Üye Adı giriniz">
                </div>
            </div>

            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">Üye Soyadı</label>
                <div class="col-sm-10">
                    <input type="text" name="UyeSoyadi" class="form-control" id="exampleInputPassword2" placeholder="Üye Soyadı giriniz">
                </div>
            </div>

            <!-- /.box-body -->
            <div class="box-footer">
                <button type="submit" class="btn btn-default">İptal Et</button>
                <input type="submit" name="name" class="btn btn-success mr-2" value="Üye Ekle" />
            </div>
            <!-- /.box-footer -->
    </form>
</div>

